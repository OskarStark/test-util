{
  "name": "localheinz/test-util",
  "description": "Provides utilities for tests.",
  "type": "library",
  "keywords": [
    "test",
    "phpunit"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "require": {
    "php": "^7.0"
  },
  "require-dev": {
    "codeclimate/php-test-reporter": "0.4.4",
    "fzaninotto/faker": "^1.0.0",
    "localheinz/php-cs-fixer-config": "~1.5.1",
    "phpunit/phpunit": "^6.2.4",
    "zendframework/zend-file": "^2.7.1"
  },
  "suggest": {
    "fzaninotto/faker": "For generating fake data in tests",
    "zendframework/zend-file": "For using assertions which traverse the filesystem to find classes"
  },
  "autoload": {
    "psr-4": {
      "Localheinz\\Test\\Util\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Localheinz\\Test\\Util\\Test\\": "test/"
    }
  }
}
