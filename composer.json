{
  "name": "localheinz/test-util",
  "type": "library",
  "description": "Provides utilities for tests.",
  "keywords": [
    "assertion",
    "faker",
    "phpunit",
    "test"
  ],
  "homepage": "https://github.com/localheinz/test-util",
  "license": "MIT",
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "fzaninotto/faker": "^1.7.1",
    "localheinz/classy": "0.3.0"
  },
  "require-dev": {
    "infection/infection": "~0.8.2",
    "localheinz/php-cs-fixer-config": "~1.14.0",
    "phpstan/phpstan": "~0.9.2",
    "phpunit/phpunit": "^6.0.0 || ^7.0.0"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Localheinz\\Test\\Util\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Localheinz\\Test\\Util\\Test\\": "test/"
    }
  },
  "support": {
    "issues": "https://github.com/localheinz/test-util/issues",
    "source": "https://github.com/localheinz/test-util"
  }
}
